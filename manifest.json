{
    "manifest_version":3,
    "name":"visionary",
    "version":"1.0",
    "description":"Make your daily browsing experience easier",
    "icons":{
        "16":"16.png",
        "48":"48.png",
        "128":"128.png"
    },
    "action":{
        "default_popup":"popup.html",
        "default_icon":{
            "16":"16.png",
            "48":"48.png",
            "128":"128.png"
        }
    },
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "history",
        "tabs",
        "storage",
        "notifications"
    ]
}